RewriteEngine On
RewriteCond %{HTTP_HOST} southbohemiaeventscalendar\.com$
RewriteRule (.*) http://www.jihoceskykalendar.cz/en/ [R=301,L]
RewriteCond %{HTTP_HOST} southbohemiaeventscalendar\.com/en-gb/$
RewriteRule (.*) http://www.jihoceskykalendar.cz/en/ [R=301,L]
RewriteCond %{REQUEST_URI} cs-cz/$
RewriteRule (.*) http://www.jihoceskykalendar.cz/cs/ [R=301,L]
RewriteCond %{REQUEST_URI} cs-cz/events$
RewriteRule (.*) http://jihoceskykalendar.cz/cs/show/category/1/akce [R=301,L]
RewriteCond %{REQUEST_URI} en-gb/$
RewriteRule (.*) http://www.jihoceskykalendar.cz/en/ [R=301,L]
RewriteCond %{REQUEST_URI} de-at/$
RewriteRule (.*) http://www.jihoceskykalendar.cz/en/ [R=301,L]
RewriteRule (.*) www/$1 [L]